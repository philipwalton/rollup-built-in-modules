{
  "name": "rollup-built-in-modules",
  "private": true,
  "version": "0.1.0",
  "description": "A demo of using rollup with built-in modules",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "glitch": "git pull -s recursive -X theirs origin master && refresh",
    "prestart": "if [ ! -d \"./public\" ]; then npm run build; fi",
    "start": "node server.js",
    "watch": "rollup -cw"
  },
  "config": {
    "publicDir": "public"
  },
  "author": {
    "name": "Philip Walton",
    "email": "philip@philipwalton.com",
    "url": "http://philipwalton.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "core-js": "^3.2.1",
    "express": "^4.18.2",
    "fs-extra": "^8.1.0",
    "kv-storage-polyfill": "^2.0.0",
    "nunjucks": "^3.2.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1"
  }
}
